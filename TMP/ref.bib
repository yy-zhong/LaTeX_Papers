  author = {Liu, Xinxin and Ma, Linyushan and Wang, Xianyu and Wu, Xia and Guo, Biao and Zhou, Lijing and Zhao, Zhen},
  date = {2024-04},
  journaltitle = {International Journal of Hydrogen Energy},
  volume = {62},
