@article{pengScreeningReversalTolerance2024,
  title = {Screening Reversal Tolerance through Rotating Disc Electrode Studies},
  author = {Peng, Ye and Choi, Ja-Yeon and Bai, Kyoung and Tian, Liliang and Pei, Katie and Zhang, Yi and Banham, Dustin},
  date = {2024-04},
  journaltitle = {International Journal of Hydrogen Energy},
