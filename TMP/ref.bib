  author = {Prinzhofer, Alain and Rigollet, Christophe and Lefeuvre, Nicolas and Françolin, Joao and Valadão De Miranda, Paulo Emilio},
  date = {2024-04},
  journaltitle = {International Journal of Hydrogen Energy},
  volume = {62},
